<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.ufv.project.controller.SearchUserController"
            prefHeight="400.0" prefWidth="600.0">
    <Label labelFor="$searchUsernameTextField"/>
    <TextField fx:id="searchUsernameTextField"/>
    <Button fx:id="searchUserButton" text="Search" onMouseClicked="#onSearchUserButtonClicked"/>
    <ProgressIndicator fx:id="progressIndicator"/>
    <ListView fx:id="userList"/>
</AnchorPane>
